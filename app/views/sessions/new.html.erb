<div class = "login_box">

	<h1> Welcome back! </h1>

	<%= form_for @user do |form| %>   
		<p> <%= form.label :email %> </p>   
		<%= form.text_field :email %> <br />
		<p> <%= form.label :password %> </p>  
		<%= form.text_field :password %> <br />      
		<p> <%= form.submit :submit %> </p>
	<% end %>

</div>